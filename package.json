{
  "name": "lifeform-searcher",
  "version": "1.0.0-1",
  "private": true,
  "homepage": "https://ironchestgames.github.io/lifeform-searcher",
  "dependencies": {
    "clamp": "^1.0.1",
    "d3-format": "^1.3.2",
    "pixi-filters": "^2.6.1",
    "pixi.js": "^4.8.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-pixi-fiber": "^0.4.9",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0"
  },
  "scripts": {
    "export:version-js": "ver=`node -pe \"require('./package.json').version\"` && echo \"module.exports = \\\"$ver\"\\\" > src/version.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "npm run export:version-js && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  }
}
